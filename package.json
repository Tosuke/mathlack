{
  "name": "mathlack",
  "version": "1.0.0",
  "description": "drawing mathematics formula in slack",
  "repository": "git@github.com:Tosuke/mathlack.git",
  "author": "Tosuke <tasukeprg@gmail.com>",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "build": "webpack && babel dist/code.js -o dist/code.js",
    "deploy": "npm run build && cd dist && gas push & cd .."
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-es3-member-expression-literals",
      "transform-es3-property-literals"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-es3-member-expression-literals": "^6.22.0",
    "babel-plugin-transform-es3-property-literals": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "corejs": "^1.0.0",
    "gas-webpack-plugin": "^0.2.0",
    "google-apps-script": "^1.2.1",
    "webpack": "^2.4.1"
  }
}
